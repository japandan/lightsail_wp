## as an option to pasting the LAMPWP script into AWS Lightsail script field for 
## building a new instance.
echo "STARTING LAMP INSTALLATION" >/root/INSTALL.LOG
yum install -y wget git >>/root/INSTALL.LOG
git clone wget https://raw.githubusercontent.com/japandan/lightsail_wp/master/LAMPWP
sh lightsail_wp/LAMPWP >>/root/INSTALL.LOG
